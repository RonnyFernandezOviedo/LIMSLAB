[Unit]
Description=Laboratory APP
After=network.target

[Service]
User=api
Group=api
WorkingDirectory=/home/api/app/src/LIMSLAb/
Enviroment="PATH=/home/api/app/venv/bin"
ExecStart=/home/api/app/venv/bin/gunicorn -w 4 -k uvicorn.workers.UvicornWorker main:felicity --bin 0.0.0.0:8000
Restart=always

[Install]
WantedBy=multi-user.target